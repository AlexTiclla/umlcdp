{
  "name": "uml-class-diagram-editor",
  "version": "1.0.0",
  "description": "UML Class Diagram Editor - Herramienta de DiseÃ±o de Base de Datos Colaborativa con IA",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:css && vite build",
    "build:prod": "npm run build:css && vite build --config vite.config.prod.js",
    "build:css": "tailwindcss -i ./css/src/styles.css -o ./css/styles.css --minify",
    "watch": "tailwindcss -i ./css/src/styles.css -o ./css/styles.css --watch",
    "dev": "vite",
    "dev:legacy": "concurrently \"npm run watch\" \"http-server -p 3000 -o\"",
    "serve": "http-server -p 3000 -o",
    "start": "npm run dev",
    "preview": "vite preview",
    "lint": "eslint js/ --ext .js",
    "lint:fix": "eslint js/ --ext .js --fix",
    "vercel-build": "node vercel-build.js"
  },
  "keywords": [
    "uml",
    "class-diagram",
    "editor",
    "collaborative",
    "ai",
    "spring-boot",
    "code-generation"
  ],
  "author": "Hassan AIT MOUSSA",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "http-server": "^14.1.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@jridgewell/set-array": "^1.2.1",
